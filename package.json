{
  "name": "Parcel boiler plate",
  "version": "2.0.1",
  "description": "A simple HTML, SCSS and JavaScript compiler",
  "source": "src/index.html",
  "scripts": {
    "clean-dist": "rm -rf dist && mkdir dist",
    "clear-cache": "rm -rf .parcel-cache",
