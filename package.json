{
  "name": "Parcel boiler plate",
  "version": "2.1.1",
  "description": "A simple HTML, SCSS and JavaScript compiler",
  "source": "src/index.html",
  "scripts": {
    "clean-dist": "rm -rf dist && mkdir dist",
    "clear-cache": "rm -rf .parcel-cache",
    "init": "npm run clean-dist && npm run clear-cache",
    "start": "npm run init && parcel",
    "build": "npm run init && parcel build",
    "build:docs": "npm run init && parcel build src/index.html --no-minify"
  },
  "keywords": [],
  "author": "Liam Champkin",
  "license": "ISC",
  "devDependencies": {
    "@parcel/plugin": "^2.13.3",
    "@parcel/transformer-css": "^2.13.3",
    "@parcel/transformer-sass": "^2.12.0",
    "@parcel/transformer-vue": "^2.12.0",
    "jsdom": "^26.0.0",
    "parcel": "^2.13.3",
    "parcel-namer-custom": "^0.2.0",
    "svgo": "^3.3.2"
  }
}